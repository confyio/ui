{
  "name": "confy-web",
  "version": "0.0.1",
  "description": "Web user interface for confy",
  "private": true,
  "namespace": "confy",
  "repository": {
    "type": "git",
    "url": "git://github.com/confyio/confy-web"
  },
  "license": "GNU AGPLv3",
  "engines": {
    "node": "6.x.x"
  },
  "scripts": {
    "docker": "docker build -t confy/web .",
    "docker:start": "docker-compose -f docker/start.yml up",
    "docker:deploy": "AAKI=$(aws configure get aws_access_key_id) ASAK=$(aws configure get aws_secret_access_key) docker-compose -f docker/deploy.yml up",
    "start": "gulp server:debug",
    "build": "gulp build:dist",
    "deploy": "gulp publish",
    "onpremise": "gulp server:onpremise"
  },
  "devDependencies": {
    "almond": "0.3.x",
    "backbone": "1.1.x",
    "backbone-computedfields": "0.0.12",
    "bootstrap": "3.3.2",
    "connect-livereload": "0.5.x",
    "es6-module-transpiler-amd-formatter": "0.3.x",
    "express": "4.14.x",
    "font-awesome": "4.6.x",
    "graceful-fs": "4.x.x",
    "gulp": "3.9.x",
    "gulp-autoprefixer": "3.1.x",
    "gulp-awspublish": "3.2.x",
    "gulp-clean-css": "2.0.x",
    "gulp-cloudfront-invalidate": "0.1.x",
    "gulp-concat": "2.6.x",
    "gulp-es6-import-validate": "0.1.x",
    "gulp-es6-module-transpiler": "0.2.x",
    "gulp-less": "3.1.x",
    "gulp-livereload": "3.8.x",
    "gulp-preprocess": "2.0.x",
    "gulp-react": "2.0.x",
    "gulp-rev-all": "0.8.x",
    "gulp-rimraf": "0.2.x",
    "gulp-sourcemaps": "1.6.x",
    "gulp-sync": "0.1.x",
    "gulp-uglify": "1.5.x",
    "gulp-useref": "3.1.x",
    "jquery": "2.2.x",
    "jquery.cookie": "1.4.x",
    "jsoneditor-confy": "3.2.x",
    "moment": "^2.19.1",
    "natives": "^1.1.3",
    "node-forge": "^0.7.1",
    "react": "0.12.x",
    "underscore": "1.8.x"
  },
  "dependencies": {}
}
